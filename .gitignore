# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/ww3d/.idea/
/ww3d/.utmp/
/ww3d/Assets/Game/Models/Textures
/ww3d/[Ll]ibrary/
/ww3d/[Tt]emp/
/ww3d/[Oo]bj/
/ww3d/[Bb]uild/
/ww3d/[Bb]uilds/
/ww3d/[Ll]ogs/
/ww3d/[Uu]ser[Ss]ettings/
/ww3d/*.log

# By default unity supports Blender asset imports, *.blend1 blender files do not need to be commited to version control.
/ww3d/*.blend1
/ww3d/*.blend1.meta

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/ww3d/[Mm]emoryCaptures/

# Recordings can get excessive in size
/ww3d/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/ww3d/[Aa]ssets/Plugins/Editor/JetBrains*
# Jetbrains Rider personal-layer settings
/ww3d/*.DotSettings.user

# Visual Studio cache directory
/ww3d/.vs/

# Gradle cache directory
/ww3d/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/ww3d/ExportedObj/
/ww3d/.consulo/
/ww3d/*.csproj
/ww3d/*.unityproj
/ww3d/*.sln
/ww3d/*.suo
/ww3d/*.tmp
/ww3d/*.user
/ww3d/*.userprefs
/ww3d/*.pidb
/ww3d/*.booproj
/ww3d/*.svd
/ww3d/*.pdb
/ww3d/*.mdb
/ww3d/*.opendb
/ww3d/*.VC.db

# Unity3D generated meta files
/ww3d/*.pidb.meta
/ww3d/*.pdb.meta
/ww3d/*.mdb.meta

# Unity3D generated file on crash reports
/ww3d/sysinfo.txt

# Mono auto generated files
/ww3d/mono_crash.*

# Builds
/ww3d/*.apk
/ww3d/*.aab
/ww3d/*.unitypackage
/ww3d/*.unitypackage.meta
/ww3d/*.app

# Crashlytics generated file
/ww3d/crashlytics-build.properties

# TestRunner generated files
/ww3d/InitTestScene*.unity*

# Addressables default ignores, before user customizations
/ww3d/ServerData
/ww3d/[Aa]ssets/StreamingAssets/aa*
/ww3d/[Aa]ssets/AddressableAssetsData/link.xml*
/ww3d/[Aa]ssets/Addressables_Temp*
# By default, Addressables content builds will generate addressables_content_state.bin
# files in platform-specific subfolders, for example:
# /Assets/AddressableAssetsData/OSX/addressables_content_state.bin
/ww3d/[Aa]ssets/AddressableAssetsData/*/*.bin*

# Visual Scripting auto-generated files
/ww3d/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Flow/UnitOptions.db
/ww3d/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Flow/UnitOptions.db.meta
/ww3d/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Core/Property Providers
/ww3d/[Aa]ssets/Unity.VisualScripting.Generated/VisualScripting.Core/Property Providers.meta

# Auto-generated scenes by play mode tests
/ww3d/[Aa]ssets/[Ii]nit[Tt]est[Ss]cene*.unity*
